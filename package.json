{
  "name": "taro3-vue3-echarts",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "templateInfo": {
    "name": "vue3-NutUI",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx,.vue src",
    "prettier": "prettier --write src/**/*.{js,ts,vue}"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "下一个雨季~ <709886167@qq.com>",
  "license": "MIT",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "eslint --fix"
    ],
    "*.ts": [
      "prettier --parser=typescript --write"
    ],
    "*.{vue,less,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@nutui/nutui-taro": "^3.1.16",
    "@tarojs/components": "3.4.2",
    "@tarojs/plugin-framework-vue3": "3.4.2",
    "@tarojs/plugin-html": "3.4.2",
    "@tarojs/runtime": "3.4.2",
    "@tarojs/taro": "3.4.2",
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@tarojs/cli": "3.4.2",
    "@tarojs/mini-runner": "3.4.2",
    "@tarojs/webpack-runner": "3.4.2",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@vue/compiler-sfc": "^3.2.26",
    "@vue/eslint-config-typescript": "^9.1.0",
    "babel-plugin-import": "^1.13.3",
    "babel-preset-taro": "3.4.2",
    "eslint": "^7.32.0",
    "eslint-config-taro": "3.4.2",
    "eslint-plugin-vue": "^8.2.0",
    "lint-staged": "^12.3.4",
    "prettier": "^2.5.1",
    "stylelint": "9.3.0",
    "taro-plugin-auto-pages": "^0.1.3",
    "typescript": "^4.5.5",
    "vue-loader": "^17.0.0",
    "yorkie": "^2.0.0"
  }
}
